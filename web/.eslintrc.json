{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "import/extensions": [
      "error",
      {
        "html": "always",
        "js": "never"
      }
    ]
  },
  "settings": {
    "import/resolver": "meteor"
  }
}
